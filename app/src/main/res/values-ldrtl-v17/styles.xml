<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="EditorAudioPlayerStyle">
        <item name="android:paddingTop">20.0dip</item>
        <item name="android:paddingBottom">@dimen/note_details_content_padding</item>
        <item name="android:paddingStart">@dimen/note_details_content_padding</item>
        <item name="android:paddingEnd">@dimen/note_details_content_padding</item>
    </style>
    <style name="IndexNoteAttachmentStyle">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:paddingBottom">2.0dip</item>
        <item name="android:paddingStart">@dimen/note_attachment_padding</item>
        <item name="android:paddingEnd">@dimen/note_attachment_padding</item>
    </style>
    <style name="IndexListLayoutStyle">
        <item name="android:paddingTop">@dimen/index_list_item_vertical_padding</item>
        <item name="android:paddingStart">@dimen/index_list_item_padding</item>
        <item name="android:paddingEnd">@dimen/index_list_item_padding</item>
    </style>
    <style name="IndexListCheckboxStyle">
        <item name="android:paddingTop">2.0dip</item>
        <item name="android:paddingEnd">@dimen/index_list_item_padding</item>
    </style>
    <style name="IndexListItemTextStyle">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/list_item_text_color</item>
        <item name="android:ellipsize">end</item>
        <item name="android:background">@null</item>
        <item name="android:maxLines">3</item>
        <item name="android:paddingEnd">@dimen/index_list_item_padding</item>
    </style>
    <style name="IndexListItemEllipseStyle">
        <item name="android:paddingBottom">@dimen/index_list_item_vertical_padding</item>
        <item name="android:text">@string/ellipse</item>
        <item name="android:layout_marginStart">24.0dip</item>
    </style>
    <style name="BrowseSectionHeaderStyle">
        <item name="android:paddingTop">12.0dip</item>
        <item name="android:paddingBottom">8.0dip</item>
        <item name="android:paddingStart">8.0dip</item>
    </style>
    <style name="WidgetHeaderTitleStyle">
        <item name="android:layout_gravity">start|center</item>
        <item name="android:paddingTop">6.0dip</item>
        <item name="android:paddingBottom">6.0dip</item>
        <item name="android:paddingStart">8.0dip</item>
    </style>
    <style name="WidgetListCheckboxStyle">
        <item name="android:paddingTop">1.0dip</item>
        <item name="android:paddingEnd">4.0dip</item>
    </style>
    <style name="EditorListItemCheckboxStyle">
        <item name="android:layout_marginStart">4.0dip</item>
    </style>
    <style name="NoteListStyle" parent="@style/ListScrollStyle">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginStart">@dimen/notes_list_side_margin</item>
        <item name="android:layout_marginEnd">@dimen/notes_list_side_margin</item>
    </style>
    <style name="EditorDeleteButtonStyle">
        <item name="android:layout_gravity">end|center|top</item>
        <item name="android:background">?selectableItemBackgroundBorderless</item>
        <item name="android:padding">4.0dip</item>
        <item name="android:layout_width">32.0dip</item>
        <item name="android:layout_height">32.0dip</item>
        <item name="android:src">@drawable/ic_material_delete_dark</item>
        <item name="android:contentDescription">@string/menu_delete</item>
        <item name="android:layout_marginEnd">16.0dip</item>
    </style>
    <style name="EditorConflictSelectionCheckmarkStyle">
        <item name="android:layout_marginTop">@dimen/editor_conflict_card_checkmark_margin</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_marginEnd">@dimen/editor_conflict_card_checkmark_margin</item>
    </style>
    <style name="WelcomeCardGotItStyle">
        <item name="android:paddingStart">8.0dip</item>
    </style>
</resources>
